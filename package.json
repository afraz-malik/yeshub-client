    "scripts": {
        "start": "export PORT=3002 && env-cmd -f .env.development react-scripts start",
        "build": "env-cmd -f .env.production react-scripts build",
        "build:stage": "env-cmd -f .env.staging react-scripts build",
        "deploy": "aws s3 --profile=yeshub cp --recursive ./build s3://yeshub-s3bucket",
        "deploy:stage": "npm run build:stage && aws s3 --profile=yeshubStage cp --debug --recursive ./build s3://uat.hub.yesdigital.org",
        "deploy:all": "npm run deploy:stage && npm run deploy",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "prettier:fix": "prettier src/* --writez`"
    },
    "eslintConfig": {
        "extends": "react-app"
